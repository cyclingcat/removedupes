<?xml version="1.0"?>

<RDF:RDF xmlns:RDF="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
         xmlns:chrome="http://www.mozilla.org/rdf/chrome#">

  <!-- packages being supplied by this jar -->
  <RDF:Seq about="urn:mozilla:package:root">
    <RDF:li resource="urn:mozilla:package:removedupes"/>
  </RDF:Seq>

  <!-- package information -->
  <RDF:Description about="urn:mozilla:package:removedupes"
        chrome:displayName="Remove Duplicate Messages (Alternate)"
        chrome:author="Eyal Rozenberg"
#expand        chrome:name="__SHORTNAME__"
        chrome:description="Finds duplicate messages in mail folders and removes them."
        chrome:extension="true">
  </RDF:Description>

  <!-- overlay info -->
  
  <!-- (note this file is only used with seamonkey; for tbird we have the chrome.manifest -->
  
  <RDF:Seq about="urn:mozilla:overlays">
    <RDF:li resource="chrome://messenger/content/mailWindowOverlay.xul"/>
    <RDF:li resource="chrome://communicator/content/pref/preftree.xul"/>
  </RDF:Seq>

  <RDF:Seq about="chrome://messenger/content/mailWindowOverlay.xul">
    <RDF:li>chrome://removedupes/content/removedupes-mailWindowOverlay.xul</RDF:li>
  </RDF:Seq>

  <RDF:Seq about="chrome://communicator/content/pref/preftree.xul">
     <RDF:li>chrome://removedupes/content/removedupes-preftree.xul</RDF:li>
  </RDF:Seq>

</RDF:RDF>
