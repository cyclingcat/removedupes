<RDF xmlns="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
     xmlns:em="http://www.mozilla.org/2004/em-rdf#">

  <Description about="urn:mozilla:install-manifest">
    
    <!-- Front End MetaData -->
    <em:id>{a300a000-5e21-4ee0-a115-9ec8f4eaa92b}</em:id>
#expand    <em:version>__VERSION__</em:version>
    <em:name>Remove Duplicate Messages (Alternate)</em:name>
    <em:description>Finds duplicate messages in mail folders and removes them.</em:description>
    <em:creator>Eyal Rozenberg</em:creator>
    <em:homepageURL>https://earendil.ath.cx/</em:homepageURL>

    <!-- Target Application this extension can install into, 
         with minimum and maximum supported versions. --> 

#ifdef MOZ_THUNDERBIRD
    <!-- Thunderbird -->
    <em:targetApplication>
     <Description>
       <em:id>{3550f703-e582-4d05-9a08-453d09bdfdc6}</em:id>
       <em:minVersion>1.5</em:minVersion>
       <em:maxVersion>2.0.*</em:maxVersion>
     </Description>
    </em:targetApplication>
#endif
#ifdef MOZ_PRETOOLKIT_SEAMONKEY
    <!-- Seamonkey -->
    <em:targetApplication>
     <Description>
       <em:id>{92650c4d-4b8e-4d2a-b7eb-24ecf4f6b63a}</em:id>
       <em:minVersion>1.0</em:minVersion>
       <em:maxVersion>1.*</em:maxVersion>
     </Description>
    </em:targetApplication>
#endif
#ifdef MOZ_TOOLKIT_SEAMONKEY
    <!-- Seamonkey -->
    <em:targetApplication>
     <Description>
       <em:id>{92650c4d-4b8e-4d2a-b7eb-24ecf4f6b63a}</em:id>
       <em:minVersion>1.0</em:minVersion>
       <em:maxVersion>2.0.*</em:maxVersion>
     </Description>
    </em:targetApplication>
#endif

    <em:iconURL>chrome://removedupes/content/removedupes.png</em:iconURL>

#ifdef MOZ_THUNDERBIRD
    <em:optionsURL>chrome://removedupes/content/removedupes-prefs.xul</em:optionsURL>
#endif

    <!-- Packages, Skins and Locales that this extension registers -->
    <em:file>
      <Description about="urn:mozilla:extension:file:removedupes.jar">
        <em:package>content/removedupes/</em:package>
        <em:locale>locale/en-US/removedupes/</em:locale>
        <em:locale>locale/de-DE/removedupes/</em:locale>
      </Description>
    </em:file>
  </Description>      
</RDF>
